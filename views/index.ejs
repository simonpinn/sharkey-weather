<html>
    <head>
    <script>
        var rawWindData = <%-JSON.stringify(windData) %>;
        var rawTempData = <%-JSON.stringify(tempData) %>;
        var rawTempData = <%-JSON.stringify(tempData) %>;
    </script>
    <link href="bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <script src="bootstrap/js/bootstrap.min.js"></script>
    <script src="d3/d3.min.js"></script>
    </head>
    <body>
    
        <div class="container-fluid">
            <div class="row">
                <h2>Wind</h2>
                <table class="table">
                    <tr>
                        <th>Recorded</th><th>Direction</th><th>Gust</th><th>Avg</th>
                    </tr>
                    <% for(var i=0;i < windData.length; i++){ %>
                    <tr>
                        <td><%=windData[i].created_date %></td>
                        <td><%=windData[i].direction %></td>
                        <td><%=windData[i].gustspeed %></td>
                        <td><%=windData[i].averagespeed %></td>
                        </tr>
                    <% } %>        
                </table>         
            </div>
            
            <div class="row">
                <h2>Temperature</h2>
                <table class="table">
                    <tr>
                        <th>Recorded</th><th>Current temp</th><th>Humidity</th><th>Humidity status</th>
                    </tr>
                    <% for(var j=0;j < tempData.length; j++){ %>
                    <tr>
                        <td><%=tempData[j].created_date %></td>
                        <td><%=tempData[j].temperature %></td>
                        <td><%=tempData[j].humidity %></td>
                        <td><%=tempData[j].humiditystatus %></td>
                        </tr>
                    <% } %>        
                </table>         
            </div>
            
            <div class="row">
                <h2>Rain</h2>
                <table class="table">
                    <tr>
                        <th>Recorded</th><th>Rainfall</th><th>Rainfall Rate</th>
                    </tr>
                    <% for(var k=0;k < rainData.length; k++){ %>
                    <tr>
                        <td><%=rainData[k].created_date %></td>
                        <td><%=rainData[k].rainfall %></td>
                        <td><%=rainData[k].rainfallrate %></td>
                        </tr>
                    <% } %>        
                </table>         
            </div>
        </div>   
        
       
    </body>
</html>